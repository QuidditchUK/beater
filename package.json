{
  "name": "beater",
  "version": "1.0.0",
  "private": true,
  "scripts": {
    "build": "babel ./src -d dist --copy-files",
    "start": "babel-node ./src",
    "serve": "npm run build && node dist/index.js",
    "prod": "node dist/index.js",
    "dev": "docker-compose -f docker-dev.yml up --build",
    "dev:api": "babel-watch src/index.js -L",
    "dev:debug": "babel-node --inspect-brk=0.0.0.0 src/index.js",
    "debug": "docker-compose -f docker-debug.yml up --build",
    "migrate": "node ./bin/migrate.js",
    "dev:startup": "bash startup.sh",
    "test": "ava",
    "test:coverage": "nyc ava"
  },
  "dependencies": {
    "axios": "0.21.1",
    "body-parser": "1.19.0",
    "connect-redis": "4.0.4",
    "cookie-parser": "1.4.5",
    "cors": "2.8.5",
    "crypto": "1.0.1",
    "express": "4.17.1",
    "express-async-handler": "1.1.4",
    "express-jwt": "5.3.3",
    "express-session": "1.17.1",
    "jsonwebtoken": "8.5.1",
    "method-override": "3.0.0",
    "migrate": "^1.6.2",
    "passport": "0.4.1",
    "passport-local": "1.0.0",
    "pg-promise": "10.8.7",
    "postmark": "^2.5.4",
    "redis": "2.8.0",
    "stripe": "^8.69.0",
    "uuidv4": "6.2.6",
    "winston": "3.2.1",
    "yup": "^0.32.0"
  },
  "devDependencies": {
    "@babel/cli": "7.12.10",
    "@babel/core": "7.12.10",
    "@babel/node": "7.12.10",
    "@babel/preset-env": "7.12.11",
    "@babel/register": "7.12.10",
    "ava": "3.15.0",
    "babel-eslint": "10.1.0",
    "babel-watch": "7.3.0",
    "dotenv": "8.2.0",
    "eslint": "7.2.0",
    "eslint-config-airbnb-base": "14.2.1",
    "eslint-plugin-import": "2.22.1",
    "husky": "4.3.7",
    "lint-staged": "9.5.0",
    "nyc": "15.1.0"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "*.js": [
      "eslint --fix"
    ]
  },
  "nyc": {
    "all": true,
    "include": [
      "src/**/*.js"
    ]
  }
}
